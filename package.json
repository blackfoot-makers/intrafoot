{
  "name": "intrafoot",
  "private": true,
  "scripts": {
    "start": "meteor run",
    "lint": "eslint . --fix",
    "pretest": "npm run lint --silent",
    "buildTravis": "tar cvf secrets.tar mup.js settings.json IntraBlackfoot.pem && sudo travis encrypt-file secrets.tar --add"
  },
  "babel": {
    "plugins": [
      "babel-plugin-transform-decorators-legacy"
    ]
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "parserOptions": {
      "allowImportExportEverywhere": true
    },
    "plugins": [
      "meteor"
    ],
    "extends": [
      "airbnb",
      "plugin:meteor/recommended",
      "prettier"
    ],
    "settings": {
      "import/resolver": "meteor"
    },
    "rules": {
      "import/no-unresolved": [
        2,
        {
          "ignore": [
            "^meteor/"
          ]
        }
      ],
      "import/extensions": [
        0
      ],
      "import/no-extraneous-dependencies": [
        0
      ],
      "comma-dangle": [
        "error",
        "only-multiline"
      ],
      "react/forbid-prop-types": [
        0
      ],
      "no-underscore-dangle": [
        0
      ],
      "react/jsx-filename-extension": [
        1,
        {
          "extensions": [
            ".js",
            ".jsx"
          ]
        }
      ],
      "linebreak-style": [
        0
      ]
    },
    "env": {
      "meteor": true,
      "node": true,
      "browser": true
    }
  },
  "dependencies": {
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-runtime": "^6.25.0",
    "bcrypt": "^1.0.2",
    "browser-filesaver": "^1.1.1",
    "classnames": "^2.2.5",
    "create-hash": "^1.1.2",
    "decko": "^1.2.0",
    "diffie-hellman": "^5.0.2",
    "history": "^4.6.3",
    "invariant": "^2.2.2",
    "jszip-utils": "^0.0.2",
    "material-design-icons": "^3.0.1",
    "material-design-lite": "^1.3.0",
    "material-ui": "^0.18.7",
    "meteor-node-stubs": "~0.2.3",
    "mongodb": "^2.2.16",
    "react": "^15.6.1",
    "react-datepicker": "^0.52.0",
    "react-dom": "^15.6.1",
    "react-mdl": "^1.10.3",
    "react-mdl-extra": "^1.4.3",
    "react-redux": "^5.0.5",
    "react-router": "^4.1.1",
    "react-router-dom": "^4.1.1",
    "react-router-redux": "^5.0.0-alpha.6",
    "react-tap-event-plugin": "^2.0.1",
    "redux": "^3.7.2",
    "redux-form": "^7.0.3",
    "redux-logger": "^3.0.6",
    "redux-persist": "^4.8.3",
    "redux-thunk": "^2.2.0",
    "tracker-component": "^1.3.21"
  },
  "devDependencies": {
    "babel-eslint": "^7.1.1",
    "eslint": "^3.15.0",
    "eslint-config-airbnb": "^13.0.0",
    "eslint-config-prettier": "^2.3.0",
    "eslint-import-resolver-meteor": "^0.3.4",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^2.2.3",
    "eslint-plugin-meteor": "^4.0.1",
    "eslint-plugin-react": "^6.8.0"
  }
}
